CREATE TABLE projects (
id VARCHAR(36) PRIMARY KEY,
 imageUrl TINYTEXT NOT NULL,
 title TINYTEXT NOT NULL,
 excerpt TINYTEXT NOT NULL,
 body TEXT NOT NULL,
 isPublished BOOL DEFAULT true,
 lastModified DATETIME NOT NULL);